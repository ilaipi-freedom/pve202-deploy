server {
    listen       80;
    server_name freedom.ilaipi.com;
    return 301 https://freedom.ilaipi.com$request_uri;
}

server {
    listen      443 ssl;
    server_name freedom.ilaipi.com;

    include /etc/nginx/snippets/ssl_params;
    include /etc/nginx/snippets/proxy_params;

    location / {
        proxy_pass https://freedom_front:80;
    }
}
