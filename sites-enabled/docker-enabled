server {
    listen       80;
    server_name docker.ilaipi.com;
    return 301 https://docker.ilaipi.com$request_uri;
}

server {
    listen      443 ssl;
    server_name docker.ilaipi.com;

    include /etc/nginx/snippets/ssl_params;
    include /etc/nginx/snippets/proxy_params;

    location / {
        proxy_pass http://192.168.5.252:30092;
    }
}
